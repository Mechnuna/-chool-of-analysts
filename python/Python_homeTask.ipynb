    "nums = [0, 27, 15, -15]\n",
    "\n",
    "target = 12\n",
    "\n",
    "\n",
    "def num_search(nums, target):\n",
    "    for i in nums:\n",
    "        i2 = target - i\n",
    "        if i2 in nums and i != i2:\n",
    "            return (nums.index(i), nums.index(i2))\n",
    "\n",
    "print(num_search(nums, target))"
   ]
